{
	"name": "webdb",
	"version": "PACKAGE_VERSION",
	"description": "Free and Efficient Database IDE",
	"repository": "https://gitlab.com/web-db/public/",
	"productName": "WebDB",
	"homepage": "https://webdb.app",
	"license": "SEE LICENSE IN https://webdb.app/licence.html",
	"main": "./electron/main.js",
	"type": "module",
	"author": {
		"name": "WebDB",
		"email": "main.webdb@gmail.com"
	},
	"scripts": {
		"dist": "electron-builder --mac && electron-builder --linux",
		"start": "NODE_ENV=production node src/index.js 2>> src/front/logs/err.log",
		"dev:back": "nodemon src/index.js 2>> src/front/logs/err.log",
		"dev:electron": "electron .",
		"lint": "eslint . --fix",
		"obfuscate": "javascript-obfuscator ./src --exclude=/front/ --options-preset low-obfuscation && cd src && find . -name '*js' -exec bash -c ' mv $0 ${0/\\-obfuscated/}' {} \\; && cd .."
	},
	"dependencies": {
		"@sentry/node": "latest",
		"axios": "latest",
		"cors": "latest",
		"dotenv": "latest",
		"electron-squirrel-startup": "latest",
		"express": "latest",
		"javascript-obfuscator": "latest",
		"mongodb": "latest",
		"multer": "latest",
		"mysql2": "latest",
		"node-port-scanner": "latest",
		"pg": "latest",
		"software-license-key": "latest",
		"ssh2": "latest",
		"tunnel-ssh": "latest"
	},
	"devDependencies": {
		"electron-builder": "24.7.0",
		"electron-nightly": "latest"
	},
	"optionalDependencies": {
		"eslint": "latest",
		"nodemon": "latest"
	},
	"build": {
		"appId": "com.webdb.app",
		"copyright": "Copyright Â© 2023 WebDB",
		"mac": {
			"icon": "electron/icon.icns",
			"category": "public.app-category.developer-tools",
			"type": "development",
			"target": [
				{
					"target": "dmg",
					"arch": "universal"
				}
			]
		},
		"linux": {
			"icon": "electron/icon.png",
			"category": "IDE",
			"target": [
				"deb"
			]
		},
		"deb": {
			"depends": [
				"gconf2",
				"gconf-service",
				"libnotify4",
				"libxtst6",
				"libnss3",
				"mysql-client",
				"postgresql-client",
				"mongodb-database-tools"
			]
		}
	}
}
