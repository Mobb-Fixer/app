{
	"name": "webdb",
	"version": "PACKAGE_VERSION",
	"scripts": {
		"dev": "ng serve",
		"extraMongo": "cd ./node_modules/mongodb/ && dts-bundle-generator -o tmp.d.ts ./mongodb.d.ts && cp -fr tmp.d.ts ../../../../../src/assets/mongo.d.ts",
		"extraBson": "cd ./node_modules/mongodb/ && dts-bundle-generator -o tmp.d.ts ./mongodb.d.ts && cp -fr tmp.d.ts ../../../../../src/assets/mongo.d.ts",
		"build": "pnpm run extraBson && pnpm run extraMongo && ng build",
		"watch": "ng build --watch --configuration development"
	},
	"dependencies": {
		"@angular-devkit/build-angular": "^14.2.11",
		"@angular/animations": "^14.3.0",
		"@angular/cdk": "^14.2.7",
		"@angular/cli": "^14.2.11",
		"@angular/common": "^14.3.0",
		"@angular/compiler": "^14.3.0",
		"@angular/compiler-cli": "^14.3.0",
		"@angular/core": "^14.3.0",
		"@angular/flex-layout": "^14.0.0-beta.41",
		"@angular/forms": "^14.3.0",
		"@angular/material": "^14.2.7",
		"@angular/platform-browser": "^14.3.0",
		"@angular/platform-browser-dynamic": "^14.3.0",
		"@angular/router": "^14.3.0",
		"@types/crypto-js": "latest",
		"@types/file-saver-es": "latest",
		"@types/js-beautify": "latest",
		"@types/marked": "^5.0.0",
		"@types/node": "latest",
		"bson": "latest",
		"crypto-js": "latest",
		"dts-bundle-generator": "7.2.0",
		"file-saver-es": "latest",
		"highlight.js": "^11.8.0",
		"highlightjs-line-numbers.js": "^2.8.0",
		"html-to-image": "latest",
		"js-beautify": "latest",
		"marked": "^5.0.5",
