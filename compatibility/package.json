{
	"name": "tests",
	"version": "1.0.0",
	"type": "module",
	"scripts": {
		"launch:webdb": "docker rm -f webdb; docker pull webdb/app; docker run --name webdb -d --restart=always --add-host=\"host.docker.internal:host-gateway\" -p 22070:22071 webdb/app",
		"test:servers": "node --test-reporter=spec --test-reporter=junit --test-reporter-destination=stdout --test-reporter-destination=./report.xml index.js",
		"test:app": "node --test-reporter=spec --test-reporter-destination=stdout app.js",
		"test:dev": "node --test --watch index.js"
	},
	"dependencies": {
		"axios": "latest",
		"form-data": "latest"
	}
}
